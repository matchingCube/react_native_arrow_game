{
	"expo": {
		"name": "AwesomeProject",
		"slug": "AwesomeProject",
		"version": "1.0.0",
		"orientation": "landscape",
		"icon": "./assets/icon.png",
		"splash": {
			"image": "./assets/logo.png",
			"resizeMode": "cover",
			"backgroundColor": "#fff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#FFFFFF"
			}
		},
		"web": {
			"favicon": "./assets/favicon.png"
		}
	}
}
